                on_release:
                # on_release is a Kivy mouse release event.
                    root.manager.current = 'transformerremoval' # Switches Kivy GUI screen to screen three.
            Button: # Creates a Button Widget instance.
                text: 'Transformer Install' #  Sets above button text property

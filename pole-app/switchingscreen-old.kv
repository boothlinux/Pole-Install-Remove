                on_release:
                # on_release is a Kivy mouse release event.
                    root.manager.current = 'transformerinstall' # Switches Kivy GUI screen to screen three.
            Button: # Creates a Button Widget instance.
                text: 'RTU Removal' #  Sets above button text property
                background_color: 0.1,0.7,1,1
                on_release:
                # on_release is a Kivy mouse release event.
                    root.manager.current = 'rturemoval' # Switches Kivy GUI screen to screen three.
            Button: # Creates a Button Widget instance.
                text: 'RTU Install' #  Sets above button text property
                background_color: 0.1,0.7,1,1
                on_release:
                # on_release is a Kivy mouse release event.
                    root.manager.current = 'rtuinstall' # Switches Kivy GUI screen to screen three.
            Button: # Creates a Button Widget instance.

<PoleRemoval>: # Identifies layout/interactivity for the FirstScreen.
    canvas:
        #Color:
            #rgba: 0,0.3,0.5,1
            #rgba: 1,1,1,1

        Rectangle:
            size: self.size
            pos: self.pos
            source: 'lineman10.png'
    GridLayout: # Creates a GridLayout for FirstScreen.
        cols: 2 # Sets column property to 1.
        SmallLabel:
            text_size: self.size
            color: 0,0,0,1 
            bold: True
            halign: 'right'
            text: "Entera Job Number (required):"
            size_hint_x: 0.3
        TextInput:
            id: entera
            size_hint_x: 0.7
        SmallLabel:
            text_size: self.size
            color: 0,0,0,1
            halign: 'right'
            bold: True
            text: "PM Order Number:"
        TextInput:
            id: PMO
        SmallLabel:
            bold: True
            text_size: self.size
            halign: 'right'
            color: 0,0,0,1
            text: "Date changed:"
        DatePicker:
            id: change
        SmallLabel:
            bold: True
            text_size: self.size
            halign: 'right'
            color: 0,0,0,1
            text: "Pole Number:"
        TextInput:
            id: polenum
        SmallLabel:
            bold: True
            text_size: self.size
            halign: 'right'
            color: 0,0,0,1
            text: "Functional Location Number:"
        TextInput:
            id: funcnum
        SmallLabel:
            bold: True
            text_size: self.size
            halign: 'right'
            color: 0,0,0,1
            text: "Location Number (TX or SW)::"
        TextInput:
            id: locnum
        SmallLabel:
            bold: True
            text_size: self.size
            halign: 'right'
            color: 0,0,0,1
            text: "City Name:"
        Spinner:
            id: cityspinner
            #background_normal: ''
            #background_color: 1,1,1,0.5
            text: "Toronto"                   #default value showed
            values: ["Toronto","Scarborough","North York","Etobicoke","East York","York"]       #list of values to show
            on_text:
                root.on_spinner_select(self.text)
        SmallLabel:
            bold: True
            text_size: self.size
            halign: 'right'
            color: 0,0,0,1
            text: "Street Number:"
        TextInput:
            id: streetnum
        SmallLabel:
            bold: True
            text_size: self.size
            halign: 'right'
            color: 0,0,0,1
            text: "Street Name:"
        TextInput:
            id: streetname
        SmallLabel:
            bold: True
            text_size: self.size
            halign: 'right'
            color: 0,0,0,1
            text: "Direction:"
        Spinner:
            id: directionspinner
            text: "North"                   #default value showed
            values: ["North","South","East","West"]       #list of values to show
            on_text:
                root.on_spinner_select(self.text)
        SmallLabel:
            bold: True
            text_size: self.size
            halign: 'right'
            color: 0,0,0,1
            text: "Street Position:"
        Spinner:
            id: streetposspinner
            text: "Boulevard"                   #default value showed
            values: ["Boulevard","Rearlot","Lane","Right of Way","Other"]       #list of values to show
            on_text:
                root.on_spinner_select(self.text)
        SmallLabel:
            bold: True
            text_size: self.size
            halign: 'right'
            color: 0,0,0,1
            text: "Status of Pole:"
        Spinner:
            id: statusspinner
            text: "New Location"                   #default value showed
            values: ["Abandoned in place","New Location","In Service","Vacant","Decomissioned"]       #list of values to show
            on_text:
                root.on_spinner_select(self.text)
        SmallLabel:
            bold: True
            text_size: self.size
            halign: 'right'
            color: 0,0,0,1
            text: "Pole height:"
        Spinner:
            id: heightspinner
            text: "40 Feet"                   #default value showed
            values: ["40 feet","45 feet","50 feet","55 feet"]       #list of values to show
            on_text:
                root.on_spinner_select(self.text)
        SmallLabel:
            bold: True
            text_size: self.size
            halign: 'right'
            color: 0,0,0,1
            text: "Pole Material:"
        Spinner:
            id: materialspinner
            text: "Wood"                   #default value showed
            values: ["Wood","Steel","Concrete","Aluminum","Other"]       #list of values to show
            on_text:
                root.on_spinner_select(self.text)
        SmallLabel:
            bold: True
            text_size: self.size
            halign: 'right'
            color: 0,0,0,1
            text: "Stub Pole?:"
        Spinner:
            id: stubspinner
            text: "No"                   #default value showed
            values: ["Yes","No"]       #list of values to show
            on_text:
                root.on_spinner_select(self.text)
        SmallLabel:
            bold: True
            text_size: self.size
            halign: 'right'
            color: 0,0,0,1
            text: "Comments?:"
        TextInput:
            id: comments
        Button:
            text: "Before Photo"
            background_color: 0.1,0.7,1,1
            on_release:
                root.show_load()
        Button:
            text: "After Photo"
            background_color: 0.1,0.7,1,1
            on_release:
                root.show_load2()
        Button:
            text: "Main Menu"
            background_color: 0.1,0.7,1,1
            on_release:
                root.manager.transition.direction = 'right' # Sets screen transition movement to right.
                root.manager.current = 'welcome_screen' # Switches Kivy GUI screen to the welcome screen.
        Button:
            text: "Make and Upload to Dropbox"
            background_color: 0.1,0.7,1,1
            on_release:
                root.pressed()
            

<LoadDialog>:
    BoxLayout:
        size: root.size
        pos: root.pos
        text_size: self.width, None
        orientation: "vertical"
        FileChooserIconView:
            id: filechooser
            #path: '/home/cbooth'
            path: '/sdcard/DCIM/Camera'

        BoxLayout:
            size_hint_y: None
            height: 30
            Button:
                text: "Cancel"
                on_release: root.cancel()

            Button:
                text: "Load"
                on_release: root.load(filechooser.path, filechooser.selection)
<LoadDialog2>:
    BoxLayout:
        size: root.size
        pos: root.pos
        text_size: self.width, None
        orientation: "vertical"
        FileChooserIconView:
            id: filechooser
            #path: '/home/cbooth/Desktop'
            path: '/sdcard/DCIM/Camera'

        BoxLayout:
            size_hint_y: None
            height: 30
            Button:
                text: "Cancel"
                on_release: root.cancel()

            Button:
                text: "Load"
                on_release: root.load2(filechooser.path, filechooser.selection)                
                
                

				text: "Main Menu"
				on_release:
					root.manager.transition.direction = 'right' # Sets screen transition movement to right.
					root.manager.current = 'welcome_screen' # Switches Kivy GUI screen to the welcome screen.
			Button:
				text: "Make and Upload to Dropbox"
				on_release:
					root.pressed()
                
                
                
                
                
